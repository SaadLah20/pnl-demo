{
  "name": "pnl-creator",
  "version": "0.0.0",
  "scripts": {
    "seed": "ts-node -P tsconfig.node.json prisma/seed.ts",
    "dev:api": "ts-node -P tsconfig.node.json src/server/index.ts",
    "dev": "vite",
    "build:api": "tsc -p tsconfig.node.json",
    "build": "vite build && npm run build:api",
    "preview": "vite preview",
    "postinstall": "prisma generate",
    "start": "prisma migrate deploy && node dist/server/index.js"

  },
  "dependencies": {
    "@heroicons/vue": "^2.0.18",
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.6",
    "docx": "^9.5.3",
    "express": "^5.2.1",
    "pinia": "^2.3.1",
    "ts-node": "^10.9.2",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/tsconfig": "^0.8.1",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "typescript": "^5.9.3",
    "vite": "^4.4.9"
  },
  "prisma": {
    "seed": "ts-node -P tsconfig.node.json prisma/seed.ts"
  }
}
